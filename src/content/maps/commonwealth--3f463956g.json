ical_location_ssim": ["Boston Public Library"], "sub_location_tsi": "Norman B. Leventhal Map Center", "identifier_uri_ss": "https://ark.digitalcommonwealth.org/ark:/50959/3f463956g", "identifier_local_other_tsim": ["06_01_004978"], "identifier_local_call_tsim": ["G1106.P5 D47 1777"], "identifier_local_barcode_tsim": ["30000003808290"], "note_date_tsim": ["This date is inferred."], "note_exhibitions_tsim": ["Exhibited: \"Charting an Empire: The Atlantic Neptune\" organized by the Norman B. Leventhal Map Center at the Boston Public Library, 2013."], "note_tsim": ["Title from published bibliography.", "Relief shown by hachures. Depths shown by soundings.", "Appears in the author's Atlantic Neptune. London : J.F.W. Des Barres, 1777-[1781]."], "edition_name_tsi": "1st state.", "scale_tsim": ["Scale [ca. 1:24,900]"], "rights_ss": "No known copyright restrictions.", "license_ss": "No known restrictions on use.", "reuse_allowed_ssi": "no restrictions", "digital_origin_ssi": "reformatted_digital", "extent_tsi": "1 chart ; 75 x 54 cm.", "abstract_tsi": "The town of Falmouth (present-day Portland, Me.) was the most important supplier of forest products in the British Empire before the American Revolutionary War. Falmouth's large harbor was well protected, was easily defendable and was adjacent to vast forests of white pine, all of which contributed to the town's commercial success. This 1777 chart of the region is the first state, or edition of the map, and it details the harbor islands, as well as inland topography, roads and settlements. By the dawn of the Revolutionary War, Britain was dependent on wood produced in the colonies, and Falmouth was a key component of the extensive lumber trade.", "publisher_tsi": "J.F.W. Des Barres", "pubplace_tsi": "London", "resource_type_manuscript_bsi": false, "type_of_resource_ssim": ["Cartographic"], "lang_term_ssim": ["English"], "publishing_state_ssi": "published", "processing_state_ssi": "complete", "destination_site_ssim": ["commonwe