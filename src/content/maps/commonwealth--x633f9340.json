, "identifier_local_call_tsim": ["G3300 1759 .H6"], "identifier_local_barcode_tsim": ["39999052509302"], "note_resp_tsim": ["accurata tabula exhibita a\u0302 Ioh. Baptista Homanno."], "note_date_tsim": ["This date is inferred."], "note_tsim": ["Relief shown pictorially.", "Hand colored.", "Includes historical notes and illustrations.", "Featured in the Faces & Places Exhibit, Kravis Center, Palm Beach, FL, since 2005. MB (BRL)"], "scale_tsim": ["Scale [ca. 1:10,000,000]."], "rights_ss": "No known copyright restrictions.", "license_ss": "No known restrictions on use.", "reuse_allowed_ssi": "no restrictions", "digital_origin_ssi": "reformatted_digital", "extent_tsi": "1 map : col. ; 56 x 64 cm.", "abstract_tsi": "This attractively decorated map by the German publisher J.B. Homann was copied almost exactly from the Delisle 1703 map, but with no attribution. Compare the geographic outlines of the islands and continental landmasses -- they are the same! The map has merely been repackaged with a new title and Latinized text and place names. For example, the southeastern United States is labeled \"Floridae\" on the Delisle map, but here is designated as \"Ludoviciana,\" the Latin term for Louisiana. In addition, there are two striking illustrations. One depicts Europeans admiring a treasure chest (the riches of the land), while the natives, identified with feathered headdresses, stand in the foreground observing the newcomers' greed! The other portrays a naval battle scene, possibly a lone British or French ship attacking the Spanish galleons. This interpretation is strongly suggested with the inclusion of the tracks of the Spanish fleets, which yearly sailed in mass to provide protection from the marauding British and French privateers.", "publisher_tsi": "[Homann Erben]", "pubplace_tsi": "Noriberg\u00e6", "issuance_tsi": "monographic", "resource_type_manuscript_bsi": false, "type_of_resource_ssim": ["Cartographic"], "lang_term_ssim": ["Latin"], "publishing_state_ssi": "published", "processing_state_ssi": "complete", "destination_site_ssim": ["commonwealth", "bpl", "nblmc", "argo"], "hosting_status_ssi": "hosted", "harvesting_status_bsi": true, "exemplary_image_ssi": "commonwealth:qb98n7846", "exemplary_image_key_base_ss": "images/commonwealth:qb98n7846", "admin_set_name_ssi": "Norman B. Leventhal Map Center Collection", "admin_set_ark_id_ssi": "commonwealth:41688024w", "institution_name_ssi": "Boston Public Library", "institution_ark_id_ssi": "commonwealth:sf268508b", "collection_name_ssim": ["American Revolutionary War-Era Maps (Collection of Distinction)", "Norman B. Leventhal Map Center Collection"], "collection_ark_id_ssim": ["commonwealth:dn39z222j", "commonwealth:41688024w"], "filenames_ssim": ["05_04_000114", "commonwealth:x633f9340_metadata"], "mods_xml_ss": "", "_version_": 1777115477583069184, "timestamp": "2023-09-15T14:49:48.259Z", "subject_geographic_sim": ["New Spain", "North America", "North and Central America"], "date_facet_yearly_itim": [1759], "score": 1.0}}